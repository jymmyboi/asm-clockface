#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/js" ]; then
  exec "$basedir/js"  "$basedir/../source-map-compactor/cli" "$@"
else 
  exec js  "$basedir/../source-map-compactor/cli" "$@"
fi
